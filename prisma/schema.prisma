generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Order {
  id   Int                   @id @default(autoincrement())
  requestFrom String //flutter or telegram
  chatID String
  chain String
  dex String
  pairAdress String
  slot String
  price String
  publickey String
  secretKeyBase58 String
  paymentMethod String
  orderStatus String
}